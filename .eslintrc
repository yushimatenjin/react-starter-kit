{
  "plugins": [
    "prettier",
    "react",
    "import"
  ],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true, "semi": false}],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false} ],
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1,
    "react/jsx-uses-react": 1
  }
}